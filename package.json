{
  "name": "angularjs-ts-starter",
  "version": "2.2.0",
  "description": "Skeleton project using Angular 1.6.6, typescript and webpack",
  "main": "app/index.js",
  "repository": "https://github.com/deonclem/angularjs-ts-starter.git",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.6",
    "@uirouter/core": "^5.0.6",
    "angular": "^1.6.6",
    "angular-animate": "^1.6.6",
    "angular-aria": "^1.6.6",
    "angular-i18n": "^1.6.6",
    "angular-material": "^1.1.5",
    "angular-messages": "^1.6.6",
    "immutable": "^3.8.2",
    "ng-redux": "^3.5.2",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@types/angular": "^1.6.32",
    "@types/angular-material": "^1.1.54",
    "@types/angular-mocks": "^1.5.11",
    "@types/angular-ui-router": "^1.1.38",
    "@types/jasmine": "^2.6.0",
    "@types/webpack": "^3.0.10",
    "@types/webpack-env": "^1.13.1",
    "angular-mocks": "^1.6.6",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "jasmine-core": "^2.5.2",
    "karma": "^1.7.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.4",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.15",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "scripts": {
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "start": "npm install && npm run-script dev",
    "dev": "node node_modules/.bin/webpack-dev-server --env.dev --content-base app --host 0.0.0.0 --no-inline",
    "build": "npm test && rm -rf dist/ && node node_modules/.bin/webpack --env.prod -p && cp app/index.html dist/index.html",
    "dist": "npm test && rm -rf dist/ && node node_modules/.bin/webpack --env.prod -p && cp app/index.html dist/index.html && node node_modules/.bin/webpack-dev-server --env.prod -p --compress --content-base dist --host 0.0.0.0 --no-inline"
  },
  "author": "deonclem",
  "license": "ISC"
}
